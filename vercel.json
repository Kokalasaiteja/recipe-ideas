{
  "version": 2,
  "builds": [
    { "src": "api/app.py", "use": "@vercel/python" },
    { "src": "templates/**/*", "use": "@vercel/static" },
    { "src": "static/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/generate", "dest": "/api/app.py" },
    { "src": "/", "dest": "/api/app.py" }
  ]
}
